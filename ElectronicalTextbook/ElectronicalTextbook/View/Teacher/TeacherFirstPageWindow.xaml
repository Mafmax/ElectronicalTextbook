<Window x:Class="ElectronicalTextbook.View.Teacher.TeacherFirstPageWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ElectronicalTextbook.View.Teacher"
        mc:Ignorable="d"
        Title="TeacherFirstPageWindow" Height="450" Width="800">
        <StackPanel>
            <Button Grid.Row="0" x:Name="materials" FontSize="30">Материалы</Button>
        <Button Grid.Row="1" x:Name="tests" FontSize="30">Тесты</Button>
        <Button Grid.Row="2" x:Name="results" FontSize="30">Результаты</Button>
        <Button Grid.Row="3" x:Name="exit" FontSize="30">В главное меню</Button>
        </StackPanel>
</Window>
